       "php": ">=5.5.9",
       "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "laravel/framework": "6.20.26",
        "predis/predis": "^1.1",
        "phpunit/phpunit": "~7.0",
        "mikey179/vfsStream": "^1.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge López Cárdenas",
            "email": "jorge.lopez@dreams.es"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dreams\\LangTranslator\\": "src"
        },
        "files": [
            "src/LangTranslatorHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
